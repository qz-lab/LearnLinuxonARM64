    "version": "0.2.0",
    "configurations": [

        {
            "type": "by-gdb",
            "request": "launch",
            "debuggerPath": "aarch64-unknown-linux-gnu-gdb",
            //"debuggerArgs": ["-ex p/x $PC"],
            "stopAtEntry" : true,
            "name": "Launch(gdb) for bare-metal",
            "program": "${workspaceRoot}/output/bare-metal/bare-metal",
            "cwd": "${workspaceRoot}",
            "remote": { 
                "enabled": true,
                "address": ":1234",
                "mode": "remote",
               // "execfile": "${workspaceRoot}/output/bare-metal/bare-metal"
            }
        }
    ]
}